pipeline {
    agent any

    stages {
        stage('GIT') {
            steps {
                git branch: 'dev',
                    url: 'https://github.com/hwafa/timesheetproject.git'
            }
        }
        
        stage('Compile Stage') {
            steps {
                sh 'mvn clean compile'
            }
        }
        
       stage('SonarQube') {
            steps {
             sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=Sonarsonar1* -Dmaven.test.skip=true';
           }
       }

      stage('Nexus') {
            steps {
             sh 'mvn deploy -Dmaven.test.skip=true';
           }
       }
           

    }
}
